    <LinearLayout
        android:id="@+id/fs_pow_04"
        android:layout_width="match_parent"
        android:layout_height="54px"
        android:layout_marginTop="498px"
        android:background="@drawable/flyaudio_systemui_bottom_button_line_bg"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="203px"
        android:id="@+id/bottom_button_bg"
        android:layout_width="match_parent"
        android:layout_height="315px"
        android:orientation="vertical"
        android:visibility="gone">
