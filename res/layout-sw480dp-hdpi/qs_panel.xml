<com.android.systemui.qs.QSContainer
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/quick_settings_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="2dp">
        
    <LinearLayout

        android:id="@+id/pow_change_color"
        android:layout_width="match_parent"
        android:layout_height="496px"
        android:orientation="vertical"
        android:background="@drawable/flyaudio_systemui_qs_container_bg">

        <com.flyaudio.view.PowerWidget
            android:id="@+id/exp_power_stat"
            android:layout_width="match_parent"
            android:layout_height="226px"
            android:paddingStart="30px"
            android:paddingEnd="30px"
            android:paddingTop="40px"/>

        <LinearLayout
            android:id="@+id/volumelayout"
            android:layout_width="match_parent"
            android:layout_height="135px"
            android:orientation="horizontal"
            android:layout_marginTop="2px"
            android:gravity="center_vertical">

            <TextView
                android:id="@+id/qs_seekbar_volume_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="24px"
                android:textColor="@android:color/white"
                android:layout_marginLeft="80px"
                android:gravity="center"
                android:text="@string/qs_seekbar_volume_text" />


            <ImageButton
                android:id="@+id/qs_seekbar_volume_dec"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="19px"
                android:layout_marginRight="18px"
                android:background="@drawable/flyaudio_systemui_volume_dec_btn_selector" />

            <SeekBar
                android:id="@+id/id_seekBar_volume"
                android:layout_width="@dimen/flyausio_seekbar_width"
                android:layout_height="wrap_content"
                android:progressDrawable="@drawable/qs_seekbar_volume"
                android:thumb="@drawable/flyaudio_systemui_bottom_seekbar_slider_bg" />

            <ImageButton
                android:id="@+id/qs_seekbar_volume_inc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20px"
                android:background="@drawable/flyaudio_systemui_volume_inc_btn_selector" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/brightnesslayout"
            android:layout_width="match_parent"
            android:layout_height="135px"
            android:orientation="horizontal"
            android:layout_marginTop="2px"
            android:gravity="center_vertical">

            <TextView
                android:id="@+id/qs_seekbar_brightness_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="24px"
                android:textColor="@android:color/white"
                android:layout_marginLeft="80px"
                android:gravity="center"
                android:text="@string/qs_seekbar_brightness_text" />


            <ImageButton
                android:id="@+id/qs_seekbar_brightness_dec"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="19px"
                android:layout_marginRight="18px"
                android:background="@drawable/flyaudio_systemui_light_dec_btn_selector" />

            <SeekBar
                android:id="@+id/id_seekBar_brightness"
                android:layout_width="@dimen/flyausio_seekbar_width"
                android:layout_height="wrap_content"
                android:progressDrawable="@drawable/qs_seekbar_brightness"
                android:thumb="@drawable/flyaudio_systemui_bottom_seekbar_slider_bg" />

            <ImageButton
                android:id="@+id/qs_seekbar_brightness_inc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20px"
                android:background="@drawable/flyaudio_systemui_light_inc_btn_selector" />


        </LinearLayout>
