<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
  
   android:layout_width="645px"
   android:layout_height="340px"
   android:id="@+id/framelayout_reset"
  >
   
    
   <TextView 
       android:id="@+id/reset_title"
       android:layout_marginLeft="150px"
       android:layout_marginTop="60px"
       android:textSize="25sp"
       android:layout_height="wrap_content"
       android:layout_width="wrap_content"
       android:text="@string/reset_title"
       android:textColor="@color/reset_title"
       
       />
    
   
 <Button 
        android:id="@+id/okLabel"
        android:layout_gravity="bottom"
        android:layout_marginLeft="143px"
        android:layout_marginBottom="48px"
        android:layout_width="164px"
        android:layout_height="68px"
        
        android:text="@string/okLabel"
        android:textSize="22sp"
        android:textAllCaps="false"
 	android:typeface="normal"
        />  
          
    <Button 
        android:id="@+id/cancelLabel"
        android:layout_gravity="bottom"
        android:layout_marginLeft="380px"
        android:layout_marginBottom="48px"
        android:layout_width="164px"
        android:layout_height="68px"
        
        android:text="@string/cancelLabel"
        android:textSize="22sp"
        android:textAllCaps="false"
 	android:typeface="normal"
        />
    

</FrameLayout>


