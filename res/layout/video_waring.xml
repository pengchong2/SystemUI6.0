<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
   android:id="@+id/fl_video_waring_bg"
   android:layout_width="660px"
   android:layout_height="340px"
  >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" 
        android:gravity="center_horizontal">

        <TextView
            android:id="@+id/reset_title"
            android:layout_width="550px"
            android:gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginTop="70px"
            android:text="@string/waring"
            android:textColor="@color/reset_title"
            android:textSize="25sp" />

        <Button
            android:id="@+id/video_okLabel"
            android:layout_width="164px"
            android:layout_height="68px"
            android:layout_marginTop="50px"
            android:text="@string/okLabel"
            android:textColor="@color/button_color"
            android:textSize="22sp" />

    </LinearLayout>

</FrameLayout>



